{
  "name": "nearby-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/types": "^7.17.0",
    "active-menu-link": "^1.1.6",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.26.0",
    "create-react-app": "^3.0.1",
    "depcheck": "^1.4.3",
    "google-map-react": "^2.1.10",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "react": "^16.8.6",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.8.6",
    "react-google-maps": "^9.4.5",
    "react-google-places-autocomplete": "^3.3.4",
    "react-modal": "^3.9.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-scroll": "^1.7.12",
    "simple-scrollspy": "^2.0.1",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rename-index": "node -e \"require('fs').rename('./build/index.html', './build/200.html', (err) => { console.log(err ? err : 'File renamed!') })\"",
    "predeploy": "npm run build && npm run rename-index",
    "deploy": "cd build && surge --domain nearby-eats-jukachu.surge.sh",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "1.18.2",
    "react-loading-skeleton": "^1.2.0",
    "surge": "^0.21.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run deploy"
    }
  }
}
